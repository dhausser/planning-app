datasource db {
  provider = "sqlite"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int     @default(autoincrement()) @id
  email    String  @unique
  key      String  @unique
  name     String
  position String?
  team     Team?   @relation(fields: [teamId], references: [id])
  teamId   Int?
}

model Team {
  id      Int    @default(autoincrement()) @id
  key     String @unique
  name    String @unique
  type    String
  members User[]
}