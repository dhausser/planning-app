{
  "key": "planning-app",
  "name": "Planning app",
  "description": "Planning app in React",
  "vendor": {
      "name": "Davy Hausser",
      "url": "https://www.github.com/dhausser"
  },
  "baseUrl": "{{localBaseUrl}}",
  "links": {
      "self": "{{localBaseUrl}}/atlassian-connect.json",
      "homepage": "{{localBaseUrl}}/atlassian-connect.json"
  },
  "authentication": {
      "type": "jwt"
  },
  "lifecycle": {
      "installed": "/installed"
  },
  "scopes": [
      "READ"
  ],
  "modules": {
      "generalPages": [
          {
              "key": "planning-page-jira",
              "location": "system.top.navigation.bar",
              "name": {
                  "value": "Planify"
              },
              "url": "/planify",
              "conditions": [{
                  "condition": "user_is_logged_in"
              }]
          }
      ]
  },
  "apiMigrations": {
      "gdpr": true
  }
}
