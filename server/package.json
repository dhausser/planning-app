{
  "name": "planning-app-server",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=13.13.0"
  },
  "scripts": {
    "start": "nodemon dist/index",
    "clean": "rm -rf dist",
    "watch": "tsc --watch",
    "build": "npm -s run clean && tsc ",
    "build:generate": "npm -s run clean && npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "postinstall": "npm -s run generate",
    "dev": "tsnd --respawn src/index.ts",
    "start:dev": "ts-node src/index.ts",
    "sample": "node data/load-sample-data.js",
    "blowitallaway": "node data/load-sample-data.js --delete",
    "script": "ts-node data/script",
    "migrate:save": "npx prisma migrate save --experimental",
    "migrate:up": "npx prisma migrate up --experimental",
    "studio": "npx prisma studio --experimental"
  },
  "dependencies": {
    "@prisma/client": "^2.20.1",
    "apollo-datasource": "^0.7.0",
    "apollo-datasource-rest": "^0.9.0",
    "apollo-server-express": "^2.14.2",
    "assert": "^2.0.0",
    "connect-redis": "^4.0.4",
    "cookie-parser": "^1.4.5",
    "csvtojson": "^2.0.10",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphql": "^14.6.0",
    "oauth-sign": "^0.9.0",
    "passport": "^0.4.1",
    "passport-oauth": "^1.0.0",
    "passport-oauth2": "^1.5.0",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@types/connect-redis": "^0.0.16",
    "@types/cookie-parser": "^1.4.2",
    "@types/express-session": "^1.17.3",
    "@types/node": "^14.14.37",
    "@types/passport": "^1.0.6",
    "@types/redis": "^2.8.28",
    "nodemon": "^2.0.7",
    "prisma": "^2.20.1",
    "ts-node": "^8.10.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^4.2.4"
  }
}
