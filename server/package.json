{
  "name": "planning-app-server",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly -r dotenv/config  src/index",
    "start": "node -r dotenv/config dist/index",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && tsc",
    "build:generate": "npm -s run clean && npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "postinstall": "npm -s run generate",
    "script": "ts-node data/script",
    "sample": "node data/load-sample-data.js",
    "blowitallaway": "node data/load-sample-data.js --delete"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-beta.2",
    "@types/mongodb": "^3.5.6",
    "apollo-datasource": "^0.7.0",
    "apollo-datasource-rest": "^0.8.0",
    "apollo-server-express": "^2.10.1",
    "assert": "^2.0.0",
    "connect-mongo": "^3.2.0",
    "csvtojson": "^2.0.10",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "graphql": "^15.0.0",
    "mongodb": "^3.5.4",
    "oauth-sign": "^0.9.0",
    "passport": "^0.4.1",
    "passport-oauth": "^1.0.0",
    "ts-node-dev": "^1.0.0-pre.44"
  },
  "devDependencies": {
    "@prisma/cli": "^2.0.0-beta.2",
    "@types/express-session": "^1.17.0",
    "@types/node": "^13.11.1",
    "@types/passport": "^1.0.3",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}