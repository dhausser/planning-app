{
  "extends": [
    "react-app",
    "prettier",
    "prettier/react"
  ],
  "rules": {
    "indent": [
      "warn",
      2
    ],
    "react/jsx-indent": [
      "warn",
      2
    ],
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 80
      }
    ]
  },
  "plugins": [
    "prettier"
  ]
}